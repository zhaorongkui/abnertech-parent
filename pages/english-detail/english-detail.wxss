/* pages/track/track.wxss */

.i-t-wrap {
  padding-bottom: 150rpx;
}

.i-t-banner {
  width: 100%;
  height: 518rpx;
  opacity: 0.9;
  background-image: linear-gradient(270deg, #4b97ff 0%, #5869ff 100%);
  border-radius: 0rpx;
  margin: 0 auto;
  position: relative;
  overflow: auto;
}

.i-t-lead {
  width: 680rpx;
  margin: 40rpx auto 0;
  background: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  border-radius: 20rpx;
  height: 100rpx;
  line-height: 40rpx;
}

.i-title-infos {
  display: flex;
  justify-content: space-around;
  flex-direction: column;
  align-items: center;
  position: relative;
  margin-top: 42rpx;
  overflow: auto;
}

.i-shuaxin {
  width: 44rpx;
  height: 68rpx;
  position: absolute;
  z-index: 99999;
  top: 0;
  right: 30rpx;
}

.i-state {
  position: absolute;
  right: 140rpx;
  top: -10rpx;
}

.i-state .i-tips {
  width: 95rpx;
  height: 34rpx;
}

.i-state text {
  position: absolute;
  left: 10rpx;
  top: 6rpx;
  color: #4489ff;
  font-size: 24rpx;
}

.i-title-infos > text {
  color: #fff;
}

.i-title-infos > text:nth-child(1) {
  font-size: 34rpx;
  width: 39%;
  margin: 0 auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
}

.i-title-infos > text:nth-child(2) {
  font-size: 22rpx;
  margin-top: 12rpx;
}

.i-title-infos view.aa {
  display: flex;
  align-items: center;
  padding: 4rpx 15rpx;
  background: rgba(93, 145, 255, 1);
  border-radius: 23px;
  border: 1px solid rgba(255, 255, 255, 1);
  font-size: 28rpx;
  margin-top: 12rpx;
  color: #fff;
}

.i-title-infos view.aa image {
  width: 28rpx;
  height: 29rpx;
  margin-right: 10rpx;
}

.i-subject {
  width: 92rpx;
  height: 36rpx;
  background: rgba(255, 255, 0, 1);
  border-radius: 0px 26px 26px 0px;
  font-size: 26rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(68, 137, 255, 1);
  text-align: center;
  line-height: 37rpx;
  position: absolute;
  left: 0;
  top: 40rpx;
}

.i-t-lead image {
  width: 41rpx;
  height: 41rpx;
}

.i-t-lead view {
  width: 80%;
  font-size: 28rpx;
  color: #999;
}

.i-t-lead view text {
  font-size: 40rpx;
  color: #fc5c3c;
  padding: 0 10rpx;
}

.i-t-null-data {
  width: 100%;
  height: 100%;
  line-height: 298rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
}

.i-t-null-line {
  width: 100%;
  height: 100%;
  line-height: 350rpx;
  text-align: center;
  color: #e5e5e5;
  font-size: 28rpx;
}

.margin-l {
  width: 214rpx;
  height: 214rpx;
  position: absolute;
}

.ll {
  left: 80rpx;
  top: 210rpx;
}

.rr {
  right: 80rpx;
  top: 210rpx;
}

.i-t-s-detail {
  width: 680rpx;
  margin: 50rpx auto 0;
  overflow: auto;
}

.i-t-s-line {
  position: relative;
  width: 680rpx;
  margin: 70rpx auto 0;
  height: 500rpx;
}

.i-t-panel {
  height: 34rpx;
}

.i-t-panel text:nth-of-type(1) {
  display: block;
  width: 8rpx;
  background: #4489ff;
  height: 32rpx;
  float: left;
  border-radius: 6rpx;
  margin-top: 1%;
}

.i-t-panel text:nth-of-type(2) {
  display: block;
  font-size: 34rpx;
  color: #333;
  float: left;
  margin-left: 20rpx;
}

.container_ {
  position: absolute;
  top: 35rpx;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  width: 95%;
  height: 80%;
  background: #fff;
  border-radius: 20rpx;
}

#pieSelect {
  width: 100%;
  height: 100%;
}

.i-t-s-types {
  width: 680rpx;
  height: 140rpx;
  border-radius: 20rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 35rpx;
}

.i-t-s-types view {
  border-right: 1px solid #eaecef;
  height: 40rpx;
}

.i-t-s-types view image {
  width: 40rpx;
  height: 40rpx;
}

.i-t-s-types view.i-t-tip image {
  width: 14rpx;
  height: 27rpx;
}

.i-t-s-types view:nth-of-type(1) {
  width: 25%;
  padding: 0 25rpx 0 20rpx;
  display: flex;
  justify-content: space-around;
}

.i-t-s-types view:nth-of-type(1) text {
  color: #333;
  font-size: 34rpx;
}

.i-t-s-types view:nth-of-type(n+2) text:nth-of-type(1) {
  font-size: 40rpx;
}

.i-t-s-types view:nth-of-type(n+2) text:nth-of-type(2) {
  color: #999;
  font-size: 22rpx;
}

.i-t-s-types view:nth-of-type(n+2) {
  display: flex;
  justify-content: center;
  flex-flow: column;
  align-items: center;
  width: 35%;
}

.i-t-s-types view:nth-of-type(n+3) {
  border-right: 0;
}

.i-t-s-types view:last-child {
  display: flex;
  justify-content: center;
  flex-flow: column;
  align-items: center;
  width: 15%;
}

.i-error-list {
  width: 100%;
  position: relative;
  top: -73rpx;
}

.i-error-list .sp-list {
  width: 82%;
  margin: 0 auto;
  height: auto;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 2px 8px 2px rgba(149, 149, 149, 0.15);
  border-radius: 20px;
  padding: 20rpx 30rpx 30px 30rpx;
  margin-bottom:25rpx;
}

.sp-title {
  display: flex;
  justify-content: flex-start;
  position: relative;
  margin-top: -20rpx;
  border-bottom: 1px solid #e5e5e5;
  padding-bottom: 30rpx;
}

.sp-title-l image {
  width: 72rpx;
  height: 100rpx;
}

.sp-title-l text {
  position: absolute;
  left: 20rpx;
  top: 10rpx;
  color: #fff;
  font-size: 50rpx;
}

.sp-title-r {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 20rpx;
  width: 80%;
}

.sp-title-r view:nth-child(1) {
  margin-top: 20rpx;
}

.sp-title-r view {
  display: flex;
  justify-content: flex-start;
  margin-top: 5rpx;
}

.sp-title-r view text:nth-child(1) {
  color: #999;
  font-size: 24rpx;
  width: 110rpx;
}

.sp-title-r view text:nth-child(2) {
  color: #333;
  font-size: 24rpx;
  width: 82%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: left;
}
.yw{
  display:flex;
  justify-content: space-between;
  align-items: center;
  width: 95%;
  margin:10rpx auto 10rpx;
}
.yw > text{
   color:#333;
   font-size:24rpx;
}
.yw > view{
  display:flex;
  align-items: center;
}
.yw > view text:nth-child(1){
   color:#999;
   font-size:24rpx;
   margin-right:5rpx;
}
.yw > view text:nth-child(2){
   color:#4489FF;
   font-size:24rpx;
}
.content {
  width: 93%;
  margin:0 auto;
  height: auto;
  background: rgba(247, 247, 247, 1);
  border-radius: 2px;
  padding: 15rpx;
  margin-top:20rpx;
  display:flex;
  justify-content: space-between;
  align-items: center;
}
.content view:nth-child(1){
  width:70%;
  word-break: break-all;
  display:flex;
  flex-direction: column;
}
.content view:nth-child(1) rich-text{
  color:#333;
  font-size:24rpx;
}
.content view:nth-child(1) text:nth-child(2){
  color:#999;
  font-size:24rpx;
}
.content > text{
  width:1px;
  height:100rpx;
  background:rgba(216,216,216,1);
}
.content view:nth-child(3){
  display:flex;
  flex-direction: column;
  align-items: center;
  margin-right:20rpx;
}
.content view:nth-child(3) text:nth-child(1){
  color:#4489FF;
  font-size:40rpx;
}
.content view:nth-child(3) text:nth-child(2){
  color:#999;
  font-size:22rpx;
}
.content1{
   width: 93%;
  margin:0 auto;
  height: auto;
  background: rgba(247, 247, 247, 1);
  border-radius: 2px;
  padding: 15rpx;
  margin-top:20rpx;
  overflow: auto;
}
.content1 > view:nth-child(1){
  color:#333;
  font-size:24rpx;
  word-break: break-all;
  height:60rpx;
  overflow:hidden;
}
.content1 > view:nth-child(1).hover{
  height:auto;
  overflow:auto;
}
.content1 view:nth-child(2){
  display:flex;
  align-items: center;
  float:right;
  margin-right:5rpx;
  margin-top:15rpx;
}
.content1 view:nth-child(2) view{
  display:flex;
  align-items: center;
}
.content1 view:nth-child(2) image{
  width:18rpx;
  height:20rpx;
}
.content1 view:nth-child(2) text{
  color:#333;
  font-size:22rpx;
  margin-right:5rpx;
}