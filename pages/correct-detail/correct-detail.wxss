  font-size: 22rpx;
}

.i-t-s-types view:nth-of-type(n+2) {
  display: flex;
  justify-content: center;
  flex-flow: column;
  align-items: center;
  width: 35%;
}

.i-t-s-types view:nth-of-type(n+3) {
  border-right: 0;
}

.i-t-s-types view:last-child {
  display: flex;
  justify-content: center;
  flex-flow: column;
  align-items: center;
  width: 15%;
}

