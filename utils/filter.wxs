function timetype(my_time) {
  var timetype;
  var EndTime = my_time;
  var date = getDate();
  var NowTime = date.getTime();
  var total_micro_second = EndTime - NowTime;
  if (total_micro_second <= 0) {
    timetype = '距离作业开始'
    return timetype;
  }else{
    timetype = '提交倒计时'
    return timetype;
  }
}

function homeworkName(type) {
