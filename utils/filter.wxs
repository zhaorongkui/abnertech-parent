function formatDate(time) {
  var date = getDate(time);
  var y = date.getFullYear();
  var m = date.getMonth() + 1;
  m = m < 10 ? ('0' + m) : m;
  var d = date.getDate();
  d = d < 10 ? ('0' + d) : d;
  var week = date.getDay();
  if (week == 0) {
    str = "周日";
  } else if (week == 1) {
    str = "周一";
  } else if (week == 2) {
    str = "周二";
  } else if (week == 3) {
    str = "周三";
  } else if (week == 4) {
    str = "周四";
  } else if (week == 5) {
    str = "周五";
  } else if (week == 6) {
    str = "周六";
  }  
  return y + '年' + m + '月' + d + '日    ' +   str;
}
function parse(str) {
  var strs = JSON.parse(str)
  return strs;
}
